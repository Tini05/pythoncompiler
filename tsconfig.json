{
  "compilerOptions": {
      "jsx": "react-jsx", // For React 17+
      "moduleResolution": "node",
      "target": "ES2015", // or "ES2020", "ESNext" if you want to use the latest features
      "lib": ["ES2015", "DOM"],  // Add ES2015 (or higher) to the lib array
      "strict": true,
      "esModuleInterop": true,
      "skipLibCheck": true
  } 
   
  ,  
  "include": [
    "src/**/*"
  ]
}
